<!DOCTYPE web-app PUBLIC '-//Sun Microsystems, Inc.//DTD
  Web Application 2.3//EN' 'http://java.sun.com/dtd/web-app_2_3.dtd'>

<web-app>

  <servlet>
    <servlet-name>gate</servlet-name>
    <servlet-class>Gate</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>auth</servlet-name>
    <servlet-class>AuthorizationChecker</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>login</servlet-name>
    <jsp-file>/login.jsp</jsp-file>
  </servlet>

  <servlet>
    <servlet-name>registrationPage</servlet-name>
    <jsp-file>/registration.jsp</jsp-file>
  </servlet>

  <servlet>
    <servlet-name>lc</servlet-name>
    <servlet-class>LoginCheck</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>registrationUtil</servlet-name>
    <servlet-class>RegistrationUtil</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>home</servlet-name>
    <servlet-class>Home</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>nikacakes</servlet-name>
    <servlet-class>Nikacakes</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>dbg</servlet-name>
    <servlet-class>Dbg</servlet-class>
  </servlet>






  <servlet-mapping>
     <servlet-name>nikacakes</servlet-name>
     <url-pattern>/n</url-pattern>
  </servlet-mapping>


  <servlet-mapping>
    <servlet-name>gate</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>registrationPage</servlet-name>
    <url-pattern>/registration</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>registrationUtil</servlet-name>
    <url-pattern>/registrationutil</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>dbg</servlet-name>
    <url-pattern>/dbg</url-pattern>
  </servlet-mapping>

</web-app>